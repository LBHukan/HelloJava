import java.util.Scanner;

public class Testt {

	public static void main(String[] args) {
		int num;
		String resul;
		
		Scanner sc = new Scanner(System.in);
		System.out.println("Digite um numero: ");
		num = sc.nextInt();
		System.out.println((num < 0)? "Negativo" : "Positivo");
		
		sc.close();
	}

}
